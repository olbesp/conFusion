{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/olbesp/conFusion.git"
  },
  "scripts": {
    "start": "npm run watch:all",
    "lite": "lite-server",
    "compile:scss": "node-sass css/styles.scss css/styles.css",
    "watch:scss": "onchange 'css/*.scss' -- npm run compile:scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "lite-server": "^2.2.2",
    "node-sass": "^4.7.2",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.9"
  }
}
